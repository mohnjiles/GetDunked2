<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jt.getdunked2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.jt.getdunked2.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jt.getdunked2.ChampsActivity"
            android:label="@string/title_activity_champs" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jt.getdunked2.MainActivity" />
        </activity>
        <activity
            android:name="com.jt.getdunked2.ChampionPage"
            android:label="@string/title_activity_champion_page" >
        </activity>
        <activity
            android:name="com.jt.getdunked2.StatsActivity"
            android:label="@string/title_activity_stats" >
        </activity>
        <activity
            android:name="com.jt.getdunked2.AbilityFragment"
            android:label="@string/title_activity_ability_fragment" >
        </activity>
        <activity
            android:name="com.jt.getdunked2.LoreFragment"
            android:label="@string/title_activity_lore_fragment" >
        </activity>
    </application>

</manifest>